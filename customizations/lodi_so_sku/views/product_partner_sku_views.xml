<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="product_partner_sku_view_form" model="ir.ui.view">
        <field name="name">product.partner.sku.view.form</field>
        <field name="model">product.partner.sku</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="product_id"/>
                        <field name="sku"/>
                        <field name="internal_product_name"/>
                        <field name="partner_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="product_partner_sku_view_tree" model="ir.ui.view">
        <field name="name">product.partner.sku.view.tree</field>
        <field name="model">product.partner.sku</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_id"/>
                <field name="sku"/>
                <field name="internal_product_name"/>
                <field name="partner_id"/>
            </tree>
        </field>
    </record>
</odoo>